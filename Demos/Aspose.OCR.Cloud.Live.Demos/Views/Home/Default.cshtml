@model LandingPageModel

@{ ViewBag.Title = "Default"; }

<div class="container-fluid header1 header2 minify-header appfamily-header">
    <div class="container">
        <div class="row">
            <div class="col-md-8">
                <h1>Perform platform independent OCR operations... </h1>
                <h4>on raster images, multi-frame TIFF or Recognition blocks for English, French, Spanish and Portuguese (and many other languages) text recognition.</h4>
            </div>
            <div class="col-md-4 tc">
                <img class="col-xs-12" src="../img/aspose_ocr-for-cloud.png" alt="Aspose.OCR Cloud" width="320" />
            </div>
        </div>
    </div>
</div>
<div class="container-fluid productfamilypage bg-white pdb-5">
    <div class="row">
        <div class="container tc">
            <div class="clearfix">&nbsp;</div>
            <div class="clearfix">&nbsp;</div>
            <h3>Aspose.OCR Cloud .NET APIs Includes</h3>
            <div class="clearfix">&nbsp;</div>
            <div class="clearfix">&nbsp;</div>

            <div class="col-lg-4 productfamily_box">
                <div class="productfamilytitle">
                    <div class="imgblock">
                        <img src="../img/aspose_conversion-app.png" alt="Aspose.OCR Cloud .NET SDK" />
                    </div>
                    <a href="/ocr/conversion">
                        <span class="spanclass">Aspose.OCR @Model.Resources["AsposeProductFor"] Cloud<em>Conversion</em></span>
                        <em>Easily process OCR operations in programming language of your choice.</em>
                    </a>
                </div>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">
    window.onload = function () {
        alert('Please set "Client Id" and "Client Secret" in appsettings.json to run Aspose.OCR Cloud .NET APIs');
    };
</script>
